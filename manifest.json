{
  "manifest_version": 3,
  "name": "Jira Tab Checker",
  "version": "1.0",
  "description": "Find Jira Tickets statuses in open tabs",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
